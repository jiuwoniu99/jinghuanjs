{
    "presets": [
        [
            "es2015",
            {
                "loose": true
            }
        ],
        "react",
        "stage-1"
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
        "transform-decorators-legacy",
        "transform-export-extensions"
    ],
    "comments": false
}
